<div id="menu">
  <h1> Menu </h1>

<%= render 'shared/category_row' %>
  <div class="row">
    <% @items.each do |item| %>
      <div class="box col-lg-3">
        <h4 class="item-title"> <%= link_to("#{item.title}", item_path(item))%></h4>
        <div class="thumbnail">
          <%= image_tag item.image.url %>
        </div>
        <h5 class="item-desc"><%= item.description %></h5>
      </div>
    <% end %>
  </div>
</div>
